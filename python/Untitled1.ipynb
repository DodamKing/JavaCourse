   ],
   "source": [
    "n = int(input('입력: '))\n",
    "sum = 0\n",
    "cnt = 0\n",
    "\n",
    "for i in range(int(n/2)+1, n+1):\n",
    "    sum += i\n",
    "    cnt += 1\n",
    "    if cnt % 5 == 0:\n",
    "        print(sum)\n",
    "        sum = 0\n",
    "    "
